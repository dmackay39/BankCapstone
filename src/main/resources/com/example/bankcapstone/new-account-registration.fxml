<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="719.0" prefWidth="712.0" spacing="20.0" style="-fx-background-color: #0E1E2B;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bankcapstone.NewAccountRegistrationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label text="New Account Registration" textAlignment="CENTER" textFill="#08b0f8">
            <font>
                <Font name="System Bold" size="48.0" />
            </font>
        </Label>
      <Label text="First Name(s)" textFill="#08b0f8">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="enterFirstNameField" maxWidth="-Infinity" prefHeight="31.0" prefWidth="500.0" />
      <Label text="Last Name" textFill="#08b0f8">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="enterLastNameField" maxWidth="-Infinity" prefHeight="31.0" prefWidth="500.0" />
        <Label text="Enter Email" textFill="#08b0f8">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TextField fx:id="enterEmailField" maxWidth="-Infinity" prefHeight="31.0" prefWidth="500.0" />
        <Label text="Enter Password" textFill="#08b0f8">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <PasswordField fx:id="enterPasswordField" maxWidth="-Infinity" prefWidth="500.0" />
      <Label text="Confirm Password" textFill="#08b0f8">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <PasswordField fx:id="confirmPasswordField" maxWidth="-Infinity" prefWidth="500.0" />
        <VBox alignment="CENTER" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button id="customerSubmitButton" fx:id="newRegistrationCancelButton" onAction="#onCancelButtonClick" prefWidth="110.0" style="-fx-background-color: #c3c4c4,linear-gradient(#d6d6d6 50%, white 100%),        radial-gradient(center 50% -40%, radius 200%, #e6e6e6 45%, rgba(230,230,230,0) 50%); -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 3, 0.0 , 0 , 1 ); -fx-background-insets: 0,1,1; -fx-background-radius: 30;" text="Back" textAlignment="CENTER" textFill="#1c3d52">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
              <Button id="customerSubmitButton" fx:id="newRegistrationSubmitButton" onAction="#onSubmitButtonClick" prefWidth="110.0" style="-fx-background-color: #c3c4c4,linear-gradient(#d6d6d6 50%, white 100%),        radial-gradient(center 50% -40%, radius 200%, #e6e6e6 45%, rgba(230,230,230,0) 50%); -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 3, 0.0 , 0 , 1 ); -fx-background-insets: 0,1,1; -fx-background-radius: 30;" text="Submit" textAlignment="CENTER" textFill="#1c3d52">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
        <Label fx:id="loginText" textFill="#f51472" />
    </children>
</VBox>
